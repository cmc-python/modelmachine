.cpu mm-3

; ввести N<=0x100 и заполнить N ячеек, начиная с 0x1000, числами от 1 до N
; вывести первые 6 ячеек
.input N
.output 0x1000, 0x1001, 0x1002, 0x1003, 0x1004, 0x1005


.asm
next:
sjg I, N, exit
instr:
move I, arr
add instr, c1, instr
add I, c1, I
jump next
exit:
halt

c1: .word 1
I: .word 1
N: .word 0

.asm 0x1000
arr: .word 0

.enter 16
