.cpu mm-r

.input a
.input b
.output x

.asm
; x = ((a * -21) % 50 - b) ** 2 == 178929
        load r1, a
        smul r1, cm21
        sdiv r1, c50
        sub r2, b
        rsmul r2, r2
        store r2, x
        halt

cm21:   .word -21
c50:    .word 50
a:x:    .word 0
b:      .word 0

.enter -123 456
