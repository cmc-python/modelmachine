.cpu mm-3

.input a, b
.output a_divides_by_b

.asm
        sdiv a, b, adivb
        jeq a_divides_by_b, c0, res1
        move c0, a_divides_by_b
        halt

res1:
        move c1, a_divides_by_b
        halt

c0:     .word 0
c1:     .word 1

a:      .word 0
b:      .word 0
adivb:  .word 0
a_divides_by_b:
        .word 0

.enter 10 3
