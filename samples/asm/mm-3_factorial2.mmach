.cpu mm-3

.input N
.output factorial

.asm
        move c1, factorial
        move N, M
        begin_loop:
        sjleq M, c1, exit
        smul factorial, M, factorial
        sub M, c1, M
        jump begin_loop
exit:   halt

N:      .word 0
factorial:
        .word 0
M:      .word 0
c1:     .word 1

.enter 6
