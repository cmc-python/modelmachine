.cpu mm-m

.input a0, a1, a2, a3
.output a0, a1, a2, a3

.asm
        rsub r4, r4
loop:   load r2, array[r4]
        add r2, cx123
        store r2, array[r4]
        add r4, c2
        comp r4, size
        sjleq loop
        halt

cx123:  .word 0x123
c2:     .word 2
size:   .word 6
a0:array:.word 0
a1:     .word 0
a2:     .word 0
a3:     .word 0

.enter 123 234 345 456
