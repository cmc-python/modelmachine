.cpu mm-1

.input a, b, c
.output D

.asm
        load b
        smul b
        store b2
        load c4
        smul a
        smul c
        store _4ac
        load b2
        sub _4ac
        store D
        halt
D:a:_4ac:
        .word 0
b:b2:   .word 0
c:      .word 0
c4:     .word 4
.enter 5 8 3
