.cpu mm-s

.input a
.input b
.output x

.asm
; x = ((a * -21) % 50 - b) ** 2 == 178929
        push a ; a
        push cm21 ; a -21
        smul ; -21a
        push c50 ; -21a 50
        sdiv ; y=-21a/50 -21a%50
        push b ; y -21a%50 b
        sub ; y -21a%50-b
        dup
        smul ; y res
        pop x
        halt

cm21:   .word -21
c50:    .word 50
a:x:    .word 0
b:      .word 0

.enter -123 456
