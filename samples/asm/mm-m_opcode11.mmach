.cpu mm-m

.output a, b, c

.asm
        addr r0, a2     ; R0 := 2
a2:     store r0, a     ; print 2
a4:     addr rf, a4     ; RF := 4
        store rf, b     ; print 4
        addr ra, a2[rf] ; RA := RF + 2 = 4 + 2 = 6
        store ra, c     ; print 6
        halt
a:      .word 0
b:      .word 0
c:      .word 0
