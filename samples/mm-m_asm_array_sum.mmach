.cpu mm-m

.input 0x100,0x102,0x104,0x106,0x108
.output sum

.asm 0x100
array: .word 0,0,0,0,0
sum: .word 0
size_word: .word 2
size_array: .word 10

.asm
    load R2, size_word
    load RF, size_array
    rsub RA, RA
    rsub RC, RC
loop:
    add RA, array[RC]
    radd RC, R2
    rcomp RC, RF
    jneq loop
    store RA, sum
    halt

.enter -1 2 3 4 5
